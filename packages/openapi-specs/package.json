{
  "name": "openapi-example-specs",
  "private": true,
  "type": "module",
  "version": "0.2.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./specs/*": "./specs/*"
  },
  "scripts": {
    "build": "tsc",
    "openapitools:ts-axios": "openapi-generator-cli generate -g typescript-axios -i specs/generic/api.yml -o out",
    "redoc:stats": "redocly stats specs/pets-modular-complex/petstore-api.yml",
    "redoc:filtered": "redocly bundle filter specs/pets-modular-complex/petstore-api.yml -o petstore-filtered --config filter-config.yaml",
    "bundler:filtered": "openapi-bundler-cli bundle specs/pets-modular-complex/petstore-api.yml --filterOut tags internal pets -o petstore-filtered/foo.yml"
  },
  "files": [
    "specs"
  ],
  "dependencies": {
    "@dasaplan/ts-sdk": "workspace:*"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.21.3",
    "@redocly/cli": "^2.10.0",
    "tsconfig": "workspace:*"
  }
}
